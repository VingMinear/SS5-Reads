{
  "swagger": "2.0",
  "info": {
    "title": "Flask API",
    "description": "API documentation",
    "version": "1.0.0"
  },
  "paths": {
    "/login": {
      "post": {
        "summary": "Create login",
        "parameters": [
          {
            "name": "email",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "password",
            "in": "body",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Login created successfully"
          }
        }
      }
    }
  }
}
